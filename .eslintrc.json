{
  "extends": ["airbnb-base", "prettier", "plugin:import/warnings"],
  "root": true,
  "plugins": ["prettier"],
  "rules": {
    "no-console": 1,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "script"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "commonjs": true
  }
}
